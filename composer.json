{
    "name": "codenom/components",
    "description": "Components for CodeIgniter 4 & CMS Mageplus",
    "keywords": [
        "CodeIgniter 4",
        "Components CodeIgniter 4",
        "MagePlus"
    ],
    "homepage": "https://github.com/codenomdev/components",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/codeigniter4/CodeIgniter4"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "codeigniter4/codeigniter4": "dev-develop",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Codenom\\Components\\": "/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "post-update-cmd": [
            "composer dump-autoload"
        ]
    }
}